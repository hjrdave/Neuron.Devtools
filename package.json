{
  "name": "neuron-devtools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "storybook:run": "storybook dev -p 6006",
    "tsc": "tsc --noEmit --watch",
    "test:watch": "vitest",
    "test": "vitest run",
    "build:package": "tsc && vite build --config package/vite.config.ts && ncp package/package.json dist/package.json && ncp package/LICENSE dist/LICENSE && ncp package/CONTRIBUTING.md dist/CONTRIBUTING.md && ncp package/README.md dist/README.md"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-select": "^5.8.0",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "bootstrap": "^5.3.2",
    "ncp": "^2.0.0"
  },
  "dependencies": {}
}
