{
  "name": "neuron-devtools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "storybook:run": "storybook dev -p 6006",
    "tsc": "tsc --noEmit --watch",
    "test:watch": "vitest",
    "test": "vitest run",
    "build:package": "tsc && vite build --config package/vite.config.ts && ncp package/package.json dist/package.json && ncp package/LICENSE dist/LICENSE && ncp package/CONTRIBUTING.md dist/CONTRIBUTING.md && ncp README.md dist/README.md",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@material-tailwind/react": "^2.1.8",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "ncp": "^2.0.0",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3",
    "react-select": "^5.8.0",
    "sass": "^1.69.7",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@sandstack/neuron": "^0.0.0-alpha.0.67"
  }
